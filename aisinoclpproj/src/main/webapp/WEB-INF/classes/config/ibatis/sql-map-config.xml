		<typeAlias alias="TaxcodeRouteEntityInfo" type="com.aisino.domain.einvoice.entity.TaxcodeRouteEntity"/>
		
    </typeAliases>
    <mappers>
        <mapper resource="com/aisino/domain/einvoice/repository/mybatis/einvoice.xml"/>
        <mapper resource="com/aisino/domain/einvoice/repository/mybatis/einvoicesub.xml"/>
        <mapper resource="config/ibatis/invUpload.xml"/>
    </mappers>
</configuration>
