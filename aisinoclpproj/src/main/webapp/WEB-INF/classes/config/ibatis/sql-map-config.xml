        <typeAlias alias="InvoiceBalanceInfo" type="com.aisino.domain.einvoice.entity.InvoiceBalance"/>
        <typeAlias alias="InvoiceEntityInfo" type="com.aisino.domain.einvoice.entity.InvoiceEntity"/>
        <typeAlias alias="EShopInfo" type="com.aisino.domain.einvoice.entity.EShopInfo"/>
        <typeAlias alias="InvoiceDetailEntityInfo" type="com.aisino.domain.einvoice.entity.InvoiceDetailEntity"/>
        <typeAlias alias="InvoiceOrderEntityInfo" type="com.aisino.domain.einvoice.entity.InvoiceOrderEntity"/>
        <typeAlias alias="InvoiceLogisticsEntityInfo" type="com.aisino.domain.einvoice.entity.InvoiceLogisticsEntity"/>
        <typeAlias alias="InvoicePaymentEntityInfo" type="com.aisino.domain.einvoice.entity.InvoicePaymentEntity"/>
        <typeAlias alias="InvoiceHeaderEntityInfo" type="com.aisino.domain.einvoice.entity.InvoiceHeaderEntity"/>
